[startup]
label=Key:
interval=30
command=./keyd.sh

[caddy]
label= 
interval=60
command=./caddy.sh

[docker]
label=
command=./docker.sh
color=#0db7ed
interval=30
markup=pango

[memory]
label=MEM
separator=true
interval=30
command="$SCRIPT_DIR/memory/memory"

[ssid]
command=$SCRIPT_DIR/ssid/ssid
interval=60

[volume]
command=$SCRIPT_DIR/volume/volume
LABEL=♪ 
interval=once
signal=10

[time]
command=date +"%H:%M | %d.%m.%Y"
interval=30
